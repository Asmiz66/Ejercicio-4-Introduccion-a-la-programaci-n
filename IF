public class Main {
    public static void main(String[] args) {
        var numeroIf = 105;

        if (numeroIf > 0) {
            System.out.println("Numero positivo");
        }
        else if (numeroIf < 0) {
            System.out.println("Numero negativo");
        }
        else {
            System.out.println("Cero");
        }
    }
}
